{
    "name": "sarge",
    "version": "0.1.0",
    "packageManager": "pnpm@9.12.2",
    "private": true,
    "scripts": {
        "dev:migrate": "npx prisma migrate dev",
        "dev:start": "next dev",
        "dev": "npm run dev:migrate && npm run dev:start",
        "build": "prisma generate && next build",
        "start": "next start",
        "lint": "eslint . --max-warnings=0",
        "lint:fix": "eslint . --fix",
        "prisma:generate": "prisma generate",
        "prisma:generate:prod": "dotenv -e .env.production.local -- prisma generate",
        "prettier:check": "prettier --check .",
        "prettier:fix": "prettier --write .",
        "build:check": "NEXT_TELEMETRY_DISABLED=1 next build",
        "db:seed": "tsx prisma/seed.ts",
        "db:seed:prod": "dotenv -e .env.production.local -- tsx prisma/seed.ts",
        "db:teardown": "tsx prisma/teardown.ts",
        "db:teardown:prod": "dotenv -e .env.production.local -- tsx prisma/teardown.ts",
        "db:reset": "npm run db:teardown && npm run db:seed",
        "prisma:seed": "prisma db seed"
    },
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    },
    "engines": {
        "node": ">= 22.0.0"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.913.0",
        "@aws-sdk/client-secrets-manager": "^3.913.0",
        "@aws-sdk/s3-request-presigner": "^3.913.0",
        "@hookform/resolvers": "^5.2.2",
        "@monaco-editor/react": "4.8.0-rc.2",
        "@prisma/client": "6.15.0",
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-dropdown-menu": "^2.1.16",
        "@radix-ui/react-separator": "^1.1.8",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-tabs": "^1.1.13",
        "@radix-ui/react-tooltip": "^1.2.8",
        "@radix-ui/react-visually-hidden": "^1.2.4",
        "@tanstack/react-table": "^8.21.3",
        "better-auth": "^1.3.34",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "lucide-react": "^0.548.0",
        "next": "15.5.7",
        "prisma": "^6.15.0",
        "react": "19.1.2",
        "react-dom": "19.1.2",
        "react-hook-form": "^7.65.0",
        "react-markdown": "^10.1.0",
        "react-resizable-panels": "^3.0.6",
        "sonner": "^2.0.7",
        "tailwind-merge": "^3.3.1",
        "zod": "^4.1.9"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "@types/node": "^22",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@typescript-eslint/eslint-plugin": "^8.43.0",
        "@typescript-eslint/parser": "^8.43.0",
        "dotenv-cli": "^11.0.0",
        "eslint": "^9",
        "eslint-config-next": "15.5.7",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-tailwindcss": "^3.18.2",
        "monaco-editor": "^0.54.0",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "tailwindcss": "^4",
        "tsx": "^4.20.6",
        "typescript": "^5"
    }
}
